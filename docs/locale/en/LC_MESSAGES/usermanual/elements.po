msgid "**N2K** - NMEA2000"
msgstr ""

#: ../../usermanual/elements.rst:17 cf032351f925400da8bc6b873d51e6b9
msgid "**183** - NMEA0183"
msgstr ""

#: ../../usermanual/elements.rst:18 eee22d8c4cba408c856693fe1cf285b5
msgid "**USB** - NMEA0183 via USB"
msgstr ""

#: ../../usermanual/elements.rst:19 b5221c0a886d4f7e84e515b11817db7e
msgid "**TCP** - NMEA0183 via TCP (WiFi)"
msgstr ""

#: ../../usermanual/elements.rst:20 a338a08201c64a09a338a65fe4c3f746
