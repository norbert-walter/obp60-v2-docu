msgstr ""

#: ../../usermanual/elements.rst:13 2bec3b7944de44529f379d938083f763
msgid "**AP** - Access Point"
msgstr ""

#: ../../usermanual/elements.rst:15 f7f92c6387684498a29d8c285845cd2f
msgid "**Zustandsanzeige aktiver Bussysteme und Komponenten**"
msgstr ""

