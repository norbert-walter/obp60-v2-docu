msgstr ""

#: ../../usermanual/configuration.rst:792 c396e8521075426b886c0c7a2f624ce0
msgid "``Blue`` - blau"
msgstr ""

#: ../../usermanual/configuration.rst:793 4ff7291bb0af45268e058b2615a3ee9c
msgid "``Aqua`` - wasser"
msgstr ""

#: ../../usermanual/configuration.rst:794 c199e8830d854eafb15609c3e9ab99c0
